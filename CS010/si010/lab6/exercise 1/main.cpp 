#include <iostream>
#include <iomanip> 
using namespace std;
int sum( int a, int b)
{
    if (a > b)
    {
        for ( b; b < a ; b++) 
        {
        b =+ b;
        }   
    }
    if (a < b)
    {
        for (a; a< b; a++)
        {
        a =+a;
        }
    }
}
int main()
{
    int a , b;
    cin >> a, b;
    cout << sum (int a, int b)

}
